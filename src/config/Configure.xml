<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.models.User" alias="User" /></typeAliases>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC"></transactionManager>
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" ></property><property
					name="url" value="jdbc:mysql://localhost:3306/mybatis" /><property name="username" value="root" /><property
					name="password" value="root" /></dataSource>
		</environment>
	</environments>
	<mappers>
		<!--power by zhou quan-->
		<mapper resource="com/models/User.xml"></mapper>
	</mappers>
</configuration> 